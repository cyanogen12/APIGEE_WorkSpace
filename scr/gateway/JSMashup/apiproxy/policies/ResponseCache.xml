<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ResponseCache async="false" continueOnError="false" enabled="true" name="ResponseCache">
    <DisplayName>ResponseCache</DisplayName>
    <CacheResource>TESTAPICACHE</CacheResource>
    <Properties/>
    <CacheKey>
        <Prefix/>
        <KeyFragment ref="proxy.pathsuffix" type="string"/>
        <KeyFragment ref="request.queryparam.cachekey" type="string"/>
    </CacheKey>
    <Scope>Exclusive</Scope>
    <ExpirySettings>
        <ExpiryDate/>
        <TimeOfDay/>
        <TimeoutInSec ref="">10</TimeoutInSec>
    </ExpirySettings>
    <SkipCacheLookup/>
    <SkipCachePopulation/>
</ResponseCache>